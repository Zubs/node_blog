<%- include('./partials/header') %>
    <div class="container pt-5">
      <h1 class="pt-5">All Posts</h1>
      <% if(blogs.length > 0) { %>
        <% blogs.forEach(blog => { %>
          <h3><%= blog.title %></h3>
          <p><%= blog.snippet %></p>
        <% }) %>
      <% } else { %>
        <h3>No Blogs Yet</h3>
      <% } %>
    </div>
<%- include('./partials/footer') %>